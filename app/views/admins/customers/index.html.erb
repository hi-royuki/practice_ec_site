<h1>会員一覧</h1>
<table>
  <thead>
    <th>会員ID</th>
    <th>氏名</th>
    <th>電話番号</th>
    <th>メールアドレス</th>
    <th>会員ステータス</th>
  </thead>
  <tbody>
    <% @customers.each do |customer| %>
    <tr>
      <td><%= customer.id %></td>
      <td>
        <%= link_to admins_customer_path(customer) do %>
          <%= customer.last_name + customer.first_name %>
        <% end %>
      </td>
      <td><%= customer.telephone_number %></td>
      <td><%= customer.email %></td>
      <td></td>
    </tr>

  </tbody>
   <% end %>
</table>